# Database
DATABASE_URL="postgresql://username:password@localhost:5432/government_matching_agent?schema=public"
NEON_DATABASE_URL="your-neon-database-url"

# Application
PORT=3001
NODE_ENV=development

# JWT
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=7d
JWT_ACCESS_SECRET=your-jwt-access-secret-key-change-this-in-production
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_SECRET=your-jwt-refresh-secret-key-change-this-in-production
JWT_REFRESH_EXPIRES_IN=7d

# OAuth - Google
# Get your credentials from: https://console.developers.google.com/
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3001/api/auth/google/callback

# OAuth - Naver
# Get your credentials from: https://developers.naver.com/apps/
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret
NAVER_CALLBACK_URL=http://localhost:3001/api/auth/naver/callback

# OAuth - Kakao
# Get your credentials from: https://developers.kakao.com/
KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret
KAKAO_CALLBACK_URL=http://localhost:3001/api/auth/kakao/callback

# Frontend
FRONTEND_URL=http://localhost:3000

# 지원사업정보 API
BIZINFO_ACCESS_KEY=your-bizinfo-access-key

# OpenAI
OPENAI_API_KEY=your-openai-api-key
